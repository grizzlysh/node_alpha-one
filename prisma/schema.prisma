  types_d                 types[]                 @relation("deleted_by")
  categories_c            categories[]            @relation("created_by")
  categories_u            categories[]            @relation("updated_by")
  categories_d            categories[]            @relation("deleted_by")
  drugs_c                 drugs[]                 @relation("created_by")
